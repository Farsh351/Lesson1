#Задание 3
for i in range(1, 101):
    if i == 1:
        print('{} Процент'.format(i))
    if i > 1 and i < 5:
        print('{} Процентa'.format(i))
    if i >= 5 and i <= 20:
        print('{} Процентов'.format(i))
    if i > 20:
        if i % 10 == 1:
            print('{} Процент'.format(i))
        if i % 10 == 2 or i % 10 == 3 or i % 10 == 4:
            print('{} Процентa'.format(i))
        elif i % 10 >= 5 and i % 10 <= 9 or i//10 == 10:
            print('{} Процентов'.format(i))
